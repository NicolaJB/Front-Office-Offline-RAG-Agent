# OpenAI API key (optional here; only used if you extend to LLM)
OPENAI_API_KEY=

# Default model for local LLM/embeddings (not currently used)
DEFAULT_MODEL=gpt-5

# Qdrant vector database URL
QDRANT_URL=http://localhost:6333

# Qdrant collection name
QDRANT_COLLECTION=fo_rag_agent

# Embedding model (for text vectors)
EMBED_MODEL=text-embedding-3-small

# Host/Port for the offline agent CLI (not a web server)
HOST=0.0.0.0
PORT=8000

# Directory for logs
LOG_DIR=./logs
